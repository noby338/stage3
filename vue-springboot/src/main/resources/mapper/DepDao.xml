<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="priv.noby.vuespringboot.dao.DepDao">
    <select id="selectAll" resultMap="dep_emp_map">
        select * from dep
    </select>
    <select id="selectByDname" resultType="priv.noby.vuespringboot.entity.Dep">
        select * from dep where dname=#{dname}
    </select>
    <resultMap id="dep_emp_map" type="dep">
        <id column="did" property="did"/>
        <result column="dname" property="dname"/>
    </resultMap>

</mapper>